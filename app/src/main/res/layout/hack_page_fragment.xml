    android:layout_height="match_parent"
    tools:context="com.example.santiagoordonez.hackuoft.HackPage.HackPageFragment">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
